{
  "name": "validis-agent",
  "version": "1.0.0",
  "description": "Natural language query interface for financial data validation and analysis",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "financial-data",
    "natural-language",
    "langchain",
    "typescript",
    "express",
    "anthropic"
  ],
  "author": "Validis",
  "license": "MIT",
  "dependencies": {
    "@langchain/anthropic": "^0.2.0",
    "@langchain/core": "^0.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "langchain": "^0.2.0",
    "mssql": "^10.0.1",
    "redis": "^4.6.8",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/mssql": "^9.1.4",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "ttypescript": "^1.5.15",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
